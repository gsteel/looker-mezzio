{
    "name": "gsteel/looker-mezzio",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "George Steel",
            "email": "george@net-glue.co.uk"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "8.2.99"
        }
    },
    "require": {
        "php": "~8.2 || ~8.3",
        "gsteel/dot": "^1.7",
        "gsteel/looker": "1.0.x-dev",
        "laminas/laminas-servicemanager": "^3.22.0 || ^4.0",
        "mezzio/mezzio-helpers": "^5.15",
        "mezzio/mezzio-template": "^2.8",
        "psr/container": "^1.1.2 || ^2.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "doctrine/coding-standard": "^12.0",
        "laminas/laminas-config-aggregator": "^1.15.0",
        "maglnet/composer-require-checker": "^4.12.0",
        "phpunit/phpunit": "^10.5.32",
        "psalm/plugin-phpunit": "^0.19.0",
        "squizlabs/php_codesniffer": "^3.10.2",
        "vimeo/psalm": "^5.25.0"
    },
    "autoload": {
        "psr-4": {
            "Looker\\Mezzio\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Looker\\Mezzio\\Test\\": "test"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:gsteel/looker.git"
        }
    ]
}
